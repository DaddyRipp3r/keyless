local Window = OrionLib:MakeWindow({Name = "Ripp3rs-Hub â”ƒ ".. scriptver, HidePremium = true, SaveConfig = true, ConfigFolder = "Ripp3rs Hub", IntroText = "Ripp3rs Hub", IntroIcon = "https://i.e-z.host/nbo558hv.png"})

