local N=game:GetService("VirtualInputManager")




Tab:AddButton({
	Name = "Maxland dupe",
	Callback = function()
        print("Loading slot")
        N:SendKeyEvent(true,"Q",false,game)
        wait(0.1) -- free land
        N:SendKeyEvent(true,"T",false,game)
        wait(0.1) -- loads the base
        N:SendKeyEvent(true,"P",false,game) -- kicks from game
        LoadSlot()
  	end    
})






